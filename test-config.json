  "testScenarios": {
    "basic_compatibility": {
      "name": "基础兼容性测试",
      "description": "测试基本的视频播放功能",
      "tests": [
        "environment_check",
        "proxy_connection",
        "direct_playback",
        "proxy_playback"
      ],
      "timeout": 30000,
      "retryAttempts": 2
    },
    "advanced_features": {
      "name": "高级功能测试",
      "description": "测试转码、WebRTC等高级功能",
      "tests": [
        "transcode_playback",
        "webrtc_playback",
        "smart_fallback",
        "adaptive_streaming"
      ],
      "timeout": 45000,
      "retryAttempts": 3
    },
    "performance_stress": {
      "name": "性能压力测试",
      "description": "测试系统在高负载下的表现",
      "tests": [
        "performance_stress",
        "memory_leak_detection",
        "cpu_usage_monitoring",
        "long_playback"
      ],
      "timeout": 120000,
      "retryAttempts": 1
    },
    "network_resilience": {
      "name": "网络弹性测试",
      "description": "测试网络中断和恢复能力",
      "tests": [
        "network_recovery",
        "bandwidth_adaptation",
        "connection_stability"
      ],
      "timeout": 60000,
      "retryAttempts": 2
    },
    "full_suite": {
      "name": "完整测试套件",
      "description": "运行所有测试项目",
      "tests": [
        "environment_check",
        "proxy_connection",
        "direct_playback",
        "proxy_playback",
        "transcode_playback",
        "webrtc_playback",
        "smart_fallback",
        "performance_stress",
        "network_recovery",
        "long_playback"
      ],
      "timeout": 180000,
      "retryAttempts": 2
    }
  },
  "serverConfiguration": {
    "proxyServer": {
      "url": "http://localhost:3001",
      "endpoints": {
        "status": "/status",
        "proxy": "/proxy",
        "transcode": "/transcode",
        "stream": "/stream",
        "cache": "/cache"
      }
    },
    "signalServer": {
      "url": "ws://localhost:8080/signal",
      "fallbackUrls": [
        "ws://localhost:8081/signal",
        "ws://127.0.0.1:8080/signal"
      ]
    },
    "stunServers": [
      "stun:stun.l.google.com:19302",
      "stun:stun1.l.google.com:19302",
      "stun:stun2.l.google.com:19302"
    ]
  },
  "testParameters": {
    "timeouts": {
      "default": 30000,
      "connection": 10000,
      "playback": 15000,
      "transcode": 60000,
      "performance": 120000
    },
    "thresholds": {
      "maxCpuUsage": 80,
      "maxMemoryIncrease": 100,
      "maxLatency": 200,
      "minSuccessRate": 0.8,
      "maxDroppedFrames": 10
    },
    "networkSimulation": {
      "scenarios": [
        {
          "name": "高速网络",
          "bandwidth": 5000000,
          "latency": 50,
          "packetLoss": 0
        },
        {
          "name": "中速网络",
          "bandwidth": 2000000,
          "latency": 100,
          "packetLoss": 0.1
        },
        {
          "name": "低速网络",
          "bandwidth": 500000,
          "latency": 200,
          "packetLoss": 0.5
        },
        {
          "name": "不稳定网络",
          "bandwidth": 1000000,
          "latency": 300,
          "packetLoss": 2.0
        }
      ]
    }
  },
  "reportConfiguration": {
    "outputFormats": ["json", "html", "csv"],
    "includeDetails": true,
    "includeScreenshots": false,
    "includePerformanceMetrics": true,
    "autoExport": true,
    "exportPath": "./test-reports/"
  },
  "debugConfiguration": {
    "enableConsoleLogging": true,
    "enablePerformanceLogging": true,
    "enableNetworkLogging": true,
    "logLevel": "info",
    "saveDebugLogs": true
  }
}